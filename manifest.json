{
  "manifest_version": 3,
  "name": "CopyAndPaste",
  "version": "1.0",
  "action":{
    "default_popup":"index.html",
    "default_icon": "copy.png"
  },
  "background":{
    "service_worker": "background.js",
    "type": "module"
  },
    "permissions": [
      "tabs",
      "storage",
      "<all_urls>",
      "scripting",
      "activeTab"
    ]
}